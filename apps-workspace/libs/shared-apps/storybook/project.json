{
  "projectType": "library",
  "sourceRoot": "libs/shared-apps/storybook/src",
  "prefix": "apps-workspace",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/shared-apps/storybook"],
      "options": {
        "jestConfig": "libs/shared-apps/storybook/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/shared-apps/storybook/**/*.ts",
          "libs/shared-apps/storybook/**/*.html"
        ]
      }
    },
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "libs/shared-apps/storybook/.storybook",
        "browserTarget": "shared-apps-storybook:build-storybook",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputDir": "dist/storybook/shared-apps-storybook",
        "configDir": "libs/shared-apps/storybook/.storybook",
        "browserTarget": "shared-apps-storybook:build-storybook",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  },
  "tags": ["scope:shared-apps", "type:ui"]
}
